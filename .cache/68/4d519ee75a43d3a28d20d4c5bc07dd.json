ion: absolute;\n  top: 0;\n  left: 0;\n  display: flex;\n  flex-flow: column;\n  justify-content: space-around;\n  align-content: center;\n  width: 100vw;\n  height: 100vh;\n  z-index: 10;\n  background: rgba(44, 62, 80, 1);\n\n  .code {\n    padding: 40px;\n    color: white;\n  }\n}\n\n.hamburger {\n  width: 100px;\n  margin: 0 !important;\n  align-self: center;\n  z-index: 11;\n  background: none !important;\n  border: none !important;\n  color: rgba(211, 84, 0, 1);\n\n  &:hover {\n    color: rgba(44, 62, 80, 1);\n  }\n}\n\n.footer {\n  grid-area: footer;\n}\n\n.body {\n  grid-area: body;\n  display: flex;\n  flex-flow: column;\n  justify-content: flex-start;\n  align-content: center;\n  overflow: scroll;\n  height: 40vh;\n}\n\n.new-msg {\n  display: flex;\n  width: 100%;\n  flex-flow: column;\n  padding: 5px 5px;\n}\n\n.msg-top {\n  grid-area: top;\n  display: flex;\n  flex-flow: row;\n  justify-content: space-between;\n  align-content: center;\n}\n\n.msg-bottom {\n  display: flex;\n  flex-direction: row-reverse;\n  grid-area: bottom;\n}\n\n.from {\n}\n.time {\n  font-size: 0.5em;\n}\n\n.content {\n  word-break: break-word;\n}\n\n.mine {\n  color: rgba(142, 68, 173, 1);\n}\n\n.friend {\n  color: rgba(22, 160, 133, 1);\n}\n\n.input {\n  width: 100%;\n  height: 120px;\n  border: 1px solid rgba(44, 62, 80, 1);\n  border-radius: 0.2em;\n  font-size: 1.2em;\n  padding: 0 12px;\n}\n\n@media screen and (min-width: 351px) {\n  .footer {\n    width: 80%;\n    margin: 0 auto;\n  }\n\n  .body > .new-msg {\n    width: 80%;\n    margin: 0 auto;\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"8815f853d1e928b04f9542ed287885c0","cacheData":{"env":{}}}